{
	"compilerOptions": {
		"skipLibCheck": true,
		"strict": true,
		"outDir": "./dist",
		"target": "es2021",
		"module": "commonjs",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"preserveConstEnums": true,
		"forceConsistentCasingInFileNames": true,
		"importHelpers": false,
		"esModuleInterop": true,
		"declaration": true,
		"resolveJsonModule": true
	},
	"include": ["solutions"]
}
